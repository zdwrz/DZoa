<%@include file="/WEB-INF/views/includes/header.jsp" %><%@include file="/WEB-INF/views/includes/navi.jsp" %><div class="panel panel-default">    <div class="panel-body">        <c:forEach var="apprv" items="${tsToApproveList.keySet()}">            Submitter :            ${apprv.submitter.userDetails.firstName}            ${apprv.submitter.userDetails.lastName}            <br>            Approver :            ${apprv.approver.userDetails.firstName}            ${apprv.approver.userDetails.lastName}            <br>            Period:            ${apprv.startMonday} - ${apprv.startMonday}            <br>            Submitted Time:            ${apprv.statusDate}            <br>            Detail:            <br>            <c:forEach var="ts" items="${tsToApproveList[apprv]}">                ${ts.slot.date} - ${ts.billCode.codeValue} - ${ts.billCode.project.name} - ${ts.value} - ${ts.comment}                <br>            </c:forEach>            <form action="${pageContext.request.contextPath}/timesheet/approval/approve" method="post">                <input type="hidden" name="${_csrf.parameterName}" value="${_csrf.token}"/>                <input type="hidden" name="approvalId" value="${apprv.id}"/>                <button type="submit">Approve</button>            </form>            <form action="${pageContext.request.contextPath}/timesheet/approval/deny" method="post">                <input type="hidden" name="${_csrf.parameterName}" value="${_csrf.token}"/>                <input type="hidden" name="approvalId" value="${apprv.id}"/>                <button type="submit">Deny</button>            </form>        </c:forEach>    </div></div><%@include file="/WEB-INF/views/includes/footer.jsp" %>